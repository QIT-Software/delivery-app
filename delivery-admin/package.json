{
  "name": "spoon-and-fork-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn update-config && react-app-rewired start",
    "build": "yarn update-config && react-app-rewired build",
    "compile": "yarn update-config && tsc -p ./tsconfig.json --noEmit",
    "lint": "eslint './**/*.{ts,tsx}'",
    "build:production": "env-cmd -f .env env-cmd -f .env.production yarn build",
    "build:staging": "env-cmd -f .env env-cmd -f .env.staging yarn build",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:push": "sh scripts/push-docker.sh",
    "graphql-generate": "graphql-codegen",
    "update-config": "ts-node --project scripts/tsconfig.json scripts/update-config.ts",
    "update-localization": "node scripts/update-localization.js",
    "deploy": "yarn build && firebase deploy"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@firebase/analytics": "^0.3.4",
    "@firebase/app": "^0.6.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@react-native-community/async-storage-backend-legacy": "^2.0.0-rc.0",
    "@react-native-community/async-storage-backend-web": "^2.0.0-rc.0",
    "@spryrocks/config-react": "^1.0.2",
    "@spryrocks/react-api": "0.1.7-dev2",
    "@spryrocks/react-auth": "^0.1.5",
    "@types/markdown-it": "^10.0.1",
    "@types/markdown-to-jsx": "^6.11.1",
    "@types/promise-queue": "^2.2.0",
    "@types/ramda": "^0.27.10",
    "@types/react-table": "^7.0.19",
    "@types/yup": "^0.29.3",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "axios": "^0.19.2",
    "axios-logger": "^2.3.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "emailjs-com": "^2.4.1",
    "firebase": "^7.15.5",
    "formik": "^2.1.4",
    "graphql": "^15.3.0",
    "history": "^4.10.1",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "markdown-it": "^11.0.0",
    "markdown-to-jsx": "^6.11.4",
    "material-ui-dropzone": "^3.3.1",
    "node-sass": "^4.14.1",
    "promise-queue": "^2.2.5",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-places-autocomplete": "^2.1.0",
    "react-i18next": "^11.4.0",
    "react-markdown-editor-lite": "^1.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-table": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "smoothscroll-anchor-polyfill": "^1.3.2",
    "smoothscroll-polyfill": "^0.4.4",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@spryrocks/eslint-config-react": "^0.1.0",
    "@types/classnames": "^2.2.10",
    "@types/node-mailjet": "^3.3.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "graphql-codegen": "^0.4.0",
    "husky": "^4.2.5",
    "react-app-rewired": "^2.1.5",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn lint"
    }
  }
}
